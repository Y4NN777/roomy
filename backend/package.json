{
  "name": "roomy-backend",
  "version": "1.0.0",
  "description": "Roomy Backend API - Intelligent roommate task management platform",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test:ai": "node utils/aiTestRunner.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ai-unit": "jest tests/services/ai.test.js --verbose --detectOpenHandles",
    "test:ai-watch": "jest tests/services/ai.test.js --watch --verbose",
    "validate-ai": "node scripts/validateAISetup.js",
    "test:coverage": "jest --coverage",
    "test:realtime": "jest tests/services/realtime.test.js --verbose",
    "test:integration": "node tests/scripts/test-realtime-integration.js",
    "test:unit": "jest tests/services/realtime.test.js",
    "test:all": "npm run test:unit && npm run test:integration",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "test:e2e": "jest tests/e2e/",
    "test:load": "jest tests/load/",
    "test:ci": "npm run test:unit && npm run test:integration"
  },
  "dependencies": {
    "@google/genai": "^1.8.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^7.4.0",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "jest": "^30.0.4",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.5.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "node-cron": "^4.2.0",
    "nodemailer": "^7.0.4",
    "socket.io": "^4.8.1",
    "supertest": "^7.1.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "nodemon": "^3.1.4",
    "socket.io-client": "^4.8.1"
  },
  "keywords": [
    "roommate",
    "task-management",
    "household",
    "ai",
    "voice-assistant",
    "nodejs",
    "express",
    "mongodb"
  ],
  "author": "Rayan BICABA and DABO Yanis Axel",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
